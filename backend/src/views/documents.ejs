        <div class="section">
            <h2>Autenticación</h2>
            
            <!-- Registro de Usuario -->
            <table>
                <tbody>
                    <tr>
                        <th>Número</th>
                        <td>1</td>
                    </tr>
                    <tr>
                        <th>Nombre</th>
                        <td>Registrar Usuario</td>
                    </tr>
                    <tr>
                        <th>Descripción</th>
                        <td>Permite registrar un nuevo usuario en la base de datos</td>
                    </tr>
                    <tr>
                        <th>URL</th>
                        <td><code>/register</code></td>
                    </tr>
                    <tr>
                        <th>Método HTTP</th>
                        <td>POST</td>
                    </tr>
                    <tr>
                        <th>Header</th>
                        <td>
                            <strong>Content-Type:</strong> application/json
                        </td>
                    </tr>
                    <tr>
                        <th>Request</th>
                        <td>
                            <pre>
{
    "fullname": "Juan Perez",
    "email": "juan@example.com",
    "password": "contraseña123"
}
                            </pre>
                        </td>
                    </tr>
                    <tr>
                        <th>Response</th>
                        <td>
                            <strong>Correcto (201):</strong><br>
                            <pre>
{
    "message": "Usuario registrado exitosamente",
    "token": "JWT_TOKEN"
}
                            </pre>
                            <strong>Errores:</strong><br>
                            400 - Campos faltantes o email ya registrado<br>
                            500 - Error interno del servidor
                        </td>
                    </tr>
                </tbody>
            </table>
            
            <!-- Login de Usuario -->
            <table>
                <tbody>
                    <tr>
                        <th>Número</th>
                        <td>2</td>
                    </tr>
                    <tr>
                        <th>Nombre</th>
                        <td>Iniciar Sesión</td>
                    </tr>
                    <tr>
                        <th>Descripción</th>
                        <td>Permite a un usuario iniciar sesión y obtener un token JWT</td>
                    </tr>
                    <tr>
                        <th>URL</th>
